        self.p = ''
        self.d = ""
    def onUnload(self):
        #puts code for box cleanup here
        ""

    def onStart(self, nInput):
        self.ok[nInput-1] = True
        bOutput = True
        for bOk in self.ok:
            bOutput = bOutput and bOk
        if( bOutput ):
            self.ok = [False]*2
            if (self.p != self.d):
                self.p = 'false'
            else:
                self.p = 'true'
            self.signalsReceived( self.p)

